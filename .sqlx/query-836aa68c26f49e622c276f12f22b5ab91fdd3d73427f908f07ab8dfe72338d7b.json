  "query": "\n        SELECT \n            f.familiar_tags,\n            f.aspirational_tags,\n            f.recent_topics,\n            u.email,\n            u.grade\n        FROM match_previews mp\n        JOIN forms f ON f.user_id = ANY(mp.candidate_ids)\n        JOIN users u ON u.id = f.user_id\n        WHERE mp.user_id = $1\n        ",
